<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.itm.link.finalproyect_ofplants">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_planta"
        android:label="Sale Of Plants"
        android:roundIcon="@mipmap/icono_planta"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProyect_OfPlants"
        android:usesCleartextTraffic="true">
        <activity android:name=".RegistroActivity"></activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu" />
        <activity android:name=".IniciarSesion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>